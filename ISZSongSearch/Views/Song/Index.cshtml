@model IEnumerable<Song>

<h2>Songs</h2>


<table class="table">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Artist</th>
    </tr>

    @foreach (var song in Model)
    {
        <tr>
            <td><a href=/Song/ViewSong/@song.ID>@song.ID</a></td>
            <td>@song.SongName</td>
            <td>@song.Artist</td>
           

        </tr>
    }
</table>

